{
  "name": "eoneopay/utils",
  "description": "Utility and helper classes",
  "keywords": ["utilities", "helpers"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "doctrine/annotations": "^1.6",
    "illuminate/support": "^5.5",
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": { "EoneoPay\\Utils\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Tests\\EoneoPay\\Utils\\": "tests/" }
  },
  "suggest": {
    "doctrine/annotations": "To use the annotation reader",
    "indigophp/doctrine-annotation-autoload": "To autoload custom doctrine annotations",
    "illuminate/support": "To use the configuration service provider in Lumen"
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true
  }
}
