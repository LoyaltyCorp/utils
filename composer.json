{
  "name": "eoneopay/utils",
  "description": "Utility and helper classes",
  "keywords": ["utilities", "helpers"],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "EoneoPay\\Utils\\": "src/",
      "Tests\\EoneoPay\\Utils\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true
  }
}
